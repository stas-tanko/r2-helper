{
  "manifest_version": 2,

  "name":    "R2-D2 Helper",
  "version": "1.0.0",
  "icons":   {
    "128": "artoo.png"
  },
  
  "content_scripts": [
    {
      "matches":  ["https://ap.www.namecheap.com/*"],
      "js":       ["dependencies.min.js", "contentscript.js"],
      "css":      ["styles.css"]
    }
  ],
  
  "permissions": [
    "declarativeContent"
  ]
}