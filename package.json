{
  "name": "r2-helper",
  "version": "1.0.0",
  "description": "Chrome Extension",
  "main": "js/index.js",
  "scripts": {
    "build": "browserify js/main.coffee --extension=.coffee --extension=.jst -o build/contentscript.js",
    "watch": "watchify js/main.coffee --extension=.coffee --extension=.jst -o build/contentscript.js"
  },
  "author": "Stanislav Tanko",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^13.0.0",
    "coffeeify": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-declare": "^0.3.0",
    "gulp-define-module": "^0.1.4",
    "gulp-handlebars": "^4.0.0",
    "gulp-uglify": "^1.5.3",
    "gulp-watch": "^4.3.5",
    "gulp-wrap": "^0.11.0",
    "jstify": "^0.14.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "ajaxq": "0.0.7",
    "backbone": "^1.3.3",
    "backbone.marionette": "^2.4.5",
    "jquery": "^2.2.3",
    "underscore": "^1.8.3"
  },
  "browserify": {
    "transform": [
      "coffeeify",
      ["jstify", { "engine": "global" }]
    ]
  }
}
